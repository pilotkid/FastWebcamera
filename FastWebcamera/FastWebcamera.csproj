<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIcon>1024px-Gnome-camera-web.svg.png</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/pilotkid/FastWebcamera</RepositoryUrl>
    <Description>Open source quick easy way to get snapshots from a webcamera. There is no license, so use it in any project as long as your respect OpenCV's license. 

This works with WinForms, do not know if it works with .net core or UWP.</Description>
    <PackageLicenseExpression />
    <Copyright>2020 Marcello Bachechi</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="OpenCvSharp4" Version="4.2.0.20200208" />
    <PackageReference Include="OpenCvSharp4.runtime.win" Version="4.2.0.20200208" />
    <PackageReference Include="OpenCvSharp4.Windows" Version="4.2.0.20200208" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\..\Desktop\CSC 217\Final Project\1024px-Gnome-camera-web.svg.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
